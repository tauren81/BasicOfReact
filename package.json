{
    "name": "settingupenvironment",
    "version": "1.0.0",
    "description": "Создание окружения для проекта React",
    "homepage": "https://github.com/tauren81/BasicOfReact#readme",
    "bugs": {
        "url": "https://github.com/tauren81/BasicOfReact/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tauren81/BasicOfReact.git"
    },
    "license": "ISC",
    "author": "PerevozchikovAS",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "prepare": "husky",
        "start": "node index.js",
        "test": "node index.js",
        "lint": "eslint --debug src/",
        "lint:write": "eslint --debug src/ --fix",
        "prettier": "prettier --write src/**/*.js"
    },
    "_moduleAliases": {
        "@common": "dist/common"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(js|jsx)": [
            "npm run lint:write",
            "git add"
        ]
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.8",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-check-file": "^3.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-react": "^7.37.4",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prettier": "3.5.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.29.0"
    },
    "dependencies": {
        "module-alias": "^2.2.3"
    }
}
